{
    "name": "apiarchitect/log",
    "description": "Log Utility ApiArchitect.",
    "keywords": ["Log", "ApiArchitect", "lumen"],
    "license": "CC BY-NC-SA",
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "guzzlehttp/psr7": "^1.3",
        "illuminate/http": "5.3.*",
        "illuminate/queue": "5.3.*",
        "laravel-doctrine/orm": "1.2.*",
        "illuminate/contracts": "5.3.*",
        "gedmo/doctrine-extensions": "^2.4",
        "symfony/psr-http-message-bridge": "^1.0",
        "zendframework/zend-diactoros": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "fzaninotto/faker": "~1.4"
    },
    "autoload": {
        "psr-4": {
            "ApiArchitect\\Log\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiArchitect\\Log\\Tests\\": "tests/"
        }
    }
